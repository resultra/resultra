{
	"packages" : {
		"jquery": {
			"pkgPrefix": "node_modules/jquery/dist",
			"jsFiles": [ "jquery.min.js" ]
		},
		"ckeditor": {
			"pkgPrefix": "node_modules/ckeditor",
			"jsFiles": [ "*.js","lang/en.js"],
			"cssFiles": ["skins/moono/*.css" ],
			"imageFiles": ["skins/moono/*.png","skins/moono/images/**"]		
		},
		"blueimp-file-upload": {
			"pkgPrefix": "node_modules/blueimp-file-upload",
			"jsFiles": [ "js/vendor/jquery.ui.widget.js","js/jquery.fileupload.js"],
			"cssFiles": ["css/jquery.fileupload.css"],
			"imageFiles": ["img/*.gif"]		
		},
		"jquery-ui": {
			"pkgPrefix": "manual_downloads/jquery-ui-1.12.1",
			"jsFiles": [ "jquery-ui.min.js"],
			"cssFiles": ["jquery-ui.min.css"],
			"imageFiles": ["images/*.png"]		
		},
		"bootstrap": {
			"pkgPrefix": "node_modules/bootstrap/dist",
			"jsFiles": [ "js/bootstrap.min.js" ],
			"cssFiles": ["css/bootstrap.min.css"],
			"fontFiles": ["fonts/**"]
		},
		"datatables": {
			"pkgPrefix": "node_modules/datatables/media",
			"jsFiles": [ "js/jquery.dataTables.min.js" ],
			"cssFiles": ["css/jquery.dataTables.min.css"],
			"imageFiles": ["images/**"]		
		},
		"datatables-resize": {
			"pkgPrefix": "manual_downloads/DataTables-ResizePlugin",
			"jsFiles": [ "dataTables.pageResize.min.js" ]
		},
		"jquery-ui-layout": {
			"pkgPrefix": "manual_downloads/jquery-ui-layout-1.4.4",
			"jsFiles": [ "jquery.layout-latest.js" ],
			"cssFiles": ["layout-default-latest.css"]	
		},
		"font-awesome": {
			"pkgPrefix": "node_modules/font-awesome",
			"cssFiles": ["css/font-awesome.min.css"],
			"fontFiles": ["fonts/**"]
		},
		"magnific-popup": {
			"pkgPrefix": "node_modules/magnific-popup/dist",
			"jsFiles": [ "jquery.magnific-popup.min.js" ],
			"cssFiles": ["magnific-popup.css"]		
		},
		"bootstrap-rating": {
			"pkgPrefix": "node_modules/bootstrap-rating",
			"jsFiles": [ "bootstrap-rating.min.js" ],
			"cssFiles": ["bootstrap-rating.css"]		
		},
		"bootstrap-switch": {
			"pkgPrefix": "node_modules/bootstrap-switch/dist",
			"jsFiles": [ "js/bootstrap-switch.min.js" ],
			"cssFiles": ["css/bootstrap3/bootstrap-switch.min.css"]		
		},
		"select2": {
			"pkgPrefix": "node_modules/select2/dist",
			"jsFiles": [ "js/select2.full.min.js" ],
			"cssFiles": ["css/select2.min.css"]		
		},
		"awesome-bootstrap-checkbox": {
			"pkgPrefix": "node_modules/awesome-bootstrap-checkbox",
			"cssFiles": ["awesome-bootstrap-checkbox.css"]		
		},
		"moment": {
			"pkgPrefix": "node_modules/moment/min",
			"jsFiles": [ "moment.min.js" ]	
		},
		"datetimepicker": {
			"pkgPrefix": "node_modules/eonasdan-bootstrap-datetimepicker/build",
			"jsFiles": [ "js/bootstrap-datetimepicker.min.js" ],
			"cssFiles": ["css/bootstrap-datetimepicker.min.css"]		
		},
		"accounting": {
			"pkgPrefix": "node_modules/accounting",
			"jsFiles": [ "accounting.min.js" ]	
		},
		"jquery-validation": {
			"pkgPrefix": "node_modules/jquery-validation/dist",
			"jsFiles": [ "jquery.validate.min.js" ]	
		},
		"d3": {
			"pkgPrefix": "node_modules/d3/build",
			"jsFiles": [ "d3.min.js" ]	
		}
	},
	"htmlFiles": ["thirdParty.html"],
	"injectPlaceholderName": "thirdPartyAssets"
}